{
  "name": "cinema",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://gflynn2049.github.io/",
  "dependencies": {
    "@mapbox/mapbox-gl-language": "^1.0.1",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "clsx": "^2.0.0",
